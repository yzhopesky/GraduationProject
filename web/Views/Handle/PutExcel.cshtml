@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>InputExcel</title>
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    <script type="text/javascript">
        function Validation() {
           
            var str
            var count = $("#count").val();
            if (count==null) {
                count = 1;
            }
            //正则表达式
            var reg = "[1-9]";
            //与输入的数字相匹配
            if (reg.match(count)) {
               
                return true;
            } else {
                alert("输入的数字不在1-9之间，请重输！");
                return false;
            }
        }
    </script>
</head>
<body>
    <form action="@Url.Action("ExportExcle", "Handle")" method="post">
        <input type="text" name="count" id="count" value="" onblur="Validation()" /><label>考试次数（1-9）</label>


        <div>
            <input type="submit" name="name" value="导出数据" />
        </div>
    </form>
</body>
</html>
